{
  "name": "saiy-coffee-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:mobile": "npm --workspace mobile run dev",
    "dev:admin": "npm --workspace admin run dev",
    "build": "npm run build -w mobile && npm run build -w admin",
    "lint": "echo 'No linter configured yet'"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11"
  }
}
